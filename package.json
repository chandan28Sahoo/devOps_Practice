{
  "name": "user_microservice",
  "version": "1.0.0",
  "description": "User Microservice",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node server/index.ts",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon --exec npx ts-node server/index.ts | pino-pretty",
    "server": "concurrently \"npm run grpc\" \"npm run start\"",
    "grpc": "ts-node server/grpc.ts",
    "client-grpc": "ts-node client/client.js",
    "client": "concurrently \"npm run client-grpc\" \"npm run dev\"",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "@sentry/node": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/google-protobuf": "^3.15.3",
    "chai-http": "^4.3.0",
    "concurrently": "^6.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "create-hmac": "^1.1.7",
    "crypto": "^1.0.1",
    "date-and-time": "^1.0.1",
    "dns2": "^2.0.1",
    "dotenv": "^8.2.0",
    "dynamoose": "^2.7.3",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "pino": "^8.8.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/node": "^14.14.22",
    "@types/node-zookeeper-client": "^0.2.8",
    "@types/nodemailer": "^6.4.4",
    "@types/pino": "^6.3.5",
    "@types/redis": "^2.8.29",
    "@types/request-promise": "^4.1.48",
    "@types/shelljs": "^0.8.8",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "supertest": "^6.0.1",
    "ts-node": "^10.8.1",
    "typescript": "4.5.4"
  }
}
